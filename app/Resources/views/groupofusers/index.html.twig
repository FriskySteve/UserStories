{% extends 'base.html.twig' %}

{% block body %}
    <h1>Groupofusers list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Groupname</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for groupOfUser in groupOfUsers %}
            <tr>
                <td><a href="{{ path('groupofusers_show', { 'id': groupOfUser.id }) }}">{{ groupOfUser.id }}</a></td>
                <td>{{ groupOfUser.groupName }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('groupofusers_show', { 'id': groupOfUser.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('groupofusers_edit', { 'id': groupOfUser.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('groupofusers_new') }}">Create a new groupOfUser</a>
        </li>
    </ul>
{% endblock %}
